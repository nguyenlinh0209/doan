<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <solid android:color="#26000000" />
            <size
                android:width="18dp"
                android:height="18dp" />
        </shape>
    </item>

    <item
        android:left="0.5dp"
        android:top="0.5dp">
        <shape android:shape="oval">
            <solid android:color="#FF3B30" />
            <size
                android:width="16dp"
                android:height="16dp" />
        </shape>
    </item>

    <item
        android:left="0.5dp"
        android:top="0.5dp">
        <shape android:shape="oval">
            <stroke
                android:width="0.5dp"
                android:color="#FFFFFF" />
            <size
                android:width="16dp"
                android:height="16dp" />
        </shape>
    </item>

</layer-list>